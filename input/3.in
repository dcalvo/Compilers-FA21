PROGRAM person;
  TYPE Person = RECORD
    --name: INTEGER;
    name: ARRAY 10 OF CHAR;
    age: INTEGER;
  END;
  TYPE Parent = RECORD
    details: Person;
    children: ARRAY 2 OF Person;
  END;
  VAR parents : ARRAY 4 OF Parent;
  VAR i, j, k : INTEGER;


BEGIN
  i := 0;
    k := 0;
      READ parents[i].details.name[k];
END.