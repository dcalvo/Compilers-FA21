
PROGRAM person;
  TYPE Person = RECORD
    name: ARRAY 10 OF CHAR;
    age: INTEGER;
  END;
  TYPE Parent = RECORD
    details: Person;
    children: ARRAY 2 OF Person;
  END;
  VAR parents : ARRAY 4 OF Parent;
  VAR i, j, k : INTEGER;


BEGIN
  i := 1;
  j := 0;
  k := 5;
  READ parents[i].children[j].name[k];
END.